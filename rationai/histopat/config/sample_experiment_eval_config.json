{
    "output_dir": "/mnt/data/home/matejg/Project/histopat/experiment_output",
    "eval_generator": "eval_gen",
    "batch_size": 32,
    "definitions": {
        "datagen": "rationai.histopat.datagens.datagens.GeneratorDatagen",
        "evaluators": [
            "rationai.histopat.training.evaluators.metrics.BinaryAccuracy",
            "rationai.histopat.training.evaluators.metrics.Precision",
            "rationai.histopat.training.evaluators.metrics.Recall",
            "rationai.histopat.training.evaluators.metrics.Specificity",
            "rationai.histopat.training.evaluators.metrics.F1"
        ]
    },
    "configurations": {
        "datagen": {
            "data_sources": {
                "_class": "rationai.histopat.datagens.datasources.HDF5DataSource",
                "_data": "predictions.h5",
                "definitions": {
                    "train_ds": {
                        "keys": ["test", "train"],
                        "names": ["test"]
                    }
                }
            },
            "generators": {
                "eval_gen": {
                    "components": {
                        "data_source": "test",
                        "sampler": "rationai.histopat.datagens.samplers.SequentialTreeSampler",
                        "augmenter": "rationai.histopat.datagens.augmenters.NoOpImageAugmenter",
                        "extractor": "rationai.histopat.datagens.extractors.GenericExtractor",
                        "generator": "rationai.histopat.datagens.generators.BaseGeneratorKeras"
                    },
                    "configurations": {
                        "sampler": {"index_levels": ["slide_name"]},
                        "augmenter": {},
                        "extractor": {
                            "return": {
                                "y_pred": [{"entry": "pred"}],
                                "y_true": [{"entry": "is_cancer", "dtype": "float"}]
                            }
                        },
                        "generator": {}
                    }
                }
            }
        },
        "evaluators": [
            {"name": "Binary Accuracy", "threshold": 0.0065, "mapping": {"y_pred": "y_pred", "y_true": "y_true"}},
            {"name": "Precision", "threshold": 0.0065, "mapping": {"y_pred": "y_pred", "y_true": "y_true"}},
            {"name": "Recall", "threshold": 0.0065, "mapping": {"y_pred": "y_pred", "y_true": "y_true"}},
            {"name": "TP", "threshold": 0.0065, "mapping": {"y_pred": "y_pred", "y_true": "y_true"}},
            {"name": "TN", "threshold": 0.0065, "mapping": {"y_pred": "y_pred", "y_true": "y_true"}},
            {"name": "FP", "threshold": 0.0065, "mapping": {"y_pred": "y_pred", "y_true": "y_true"}},
            {"name": "FN", "threshold": 0.0065, "mapping": {"y_pred": "y_pred", "y_true": "y_true"}}
        ]
    }
}